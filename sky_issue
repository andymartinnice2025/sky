function initializeDataLayerHelper() {
  // Check if DataLayerHelper is defined or not every 100ms.
  if (typeof DataLayerHelper === "undefined") {
    _satellite.logger.warn("DataLayerHelper is not available yet. Retrying in 100ms...");
    setTimeout(initializeDataLayerHelper, 100);
    return;
  }
  
  try {
    // Check if window.skyDataLayer exists
    if (!window.skyDataLayer) {
      _satellite.logger.warn("window.skyDataLayer is not defined, initializing an empty array.");
      window.skyDataLayer = [];
    }

    // Define the callback function to handle changes in the data layer
    var handleDataLayerChange = function (model, message) {
      // Ensure 'extensionGoogleDataLayer.dataLayerHelper' is initialized properly
      window.extensionGoogleDataLayer.dataLayerHelper = helper || this;

      // Extract event name from the message
      var eventName = message && message.event;
      var eventNameIdentifier = "skyDataLayer-" + eventName;

      _satellite.logger.info("Dispatching Direct Call for: " + eventNameIdentifier);
      // fire the direct call and pass respective data layer associated with the event.
      _satellite.track(eventNameIdentifier, message);
    };

    // Initialize the DataLayerHelper
    var helper = new DataLayerHelper(window.skyDataLayer, {
      listener: handleDataLayerChange, // Add the callback to listen to changes in the data layer
      listenToPast: true, // Ensure past messages are also captured
    });
  } catch (e) {
    _satellite.logger.error("Error initializing DataLayerHelper:", e);
  }
}

// calling the function to initialize the DataLayerHelper
initializeDataLayerHelper();
