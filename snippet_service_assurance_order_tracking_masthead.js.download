function setEventPreviousForMobile(dataTo){
 
  var currentPrev = document.querySelector('#' + dataTo + '').querySelector('.opty-ctn-previous-mobile');
  if(currentPrev){
  
   currentPrev.addEventListener('click', function(prev) {
     
     var listLinks =  document.querySelectorAll('#' + dataTo + ' '+ '.opty-breadcrumbs-wrapper .opty-breadcrumbs-desktop-wrapper a');
   
     if(listLinks.length > 0){
     listLinks[listLinks.length -1].click();
     
     }
   });
  }
  
  
}

function addStepEvents() {

    //Steps  
    document.querySelectorAll('.opty-clickable-elem').forEach(function(elem) {
        elem.addEventListener('click', function(cta) {
            var dataTo = cta.target.getAttribute('data-to');

            if (dataTo) {
                document.querySelectorAll('.opty-tree-node').forEach(function(screen) {
                    screen.classList.add('opty-hidden-tree-screen');
                });
                document.querySelector('#' + dataTo + '').classList.remove('opty-hidden-tree-screen');
                setEventPreviousForMobile(dataTo);
              
              
              
              
              
            }



        });
    });
  


    //Contact- Phone
    if (document.querySelector('#opty-call-revealer')) {

        document.querySelector('#opty-call-revealer').addEventListener('click', function(cta) {
          numberChangeTest();
            document.querySelector('.opty-phone-num').classList.remove('optycall-toggle');
            cta.target.classList.add('optycall-toggle');


        });

    }


}
